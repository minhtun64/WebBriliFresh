@model WebBriliFresh.Models.Stock

@{
    ViewData["Title"] = "Details";
}

<h1>Thông tin chi tiết</h1>

<div>
    <h4>Kho</h4>
    <hr />
    <dl class="row">  
        <dt class = "col-sm-2">
            Cửa hàng
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Store.StoreId)
        </dd>
        <dt class="col-sm-2">
            Sản phẩm
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Pro.ProId)
        </dd>
        <dt class="col-sm-2">
           Số lượng
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Quantity)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Sửa", "Edit", new { id = Model.StoreId, id1 = Model.ProId }) |

    <a asp-action="Index" class="btn btn-success btn-tone m-r-5">Quay lại Danh sách</a>
</div>
